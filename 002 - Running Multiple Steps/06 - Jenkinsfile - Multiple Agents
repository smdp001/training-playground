pipeline {
    agent none
    stages {
    	stage('maven') {
		agent {
			docker {
				image 'maven:latest'
			}
		} 
		steps {
			sh 'mvn --version'
		}
	}
	stage('npm') {
		agent {
			docker {
				image 'node:latest' 
			}
		} 
		steps {
			sh 'npm --version'
		}	
	}
    }
}
